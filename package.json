{
  "name": "@hregibo/pg-dbc",
  "version": "0.1.1",
  "description": "Basic Postgresql query simplifier, using types",
  "main": "./build/index.js",
  "types": "./build/index.d.ts",
  "scripts": {
    "build": "npx tsc",
    "start": "node ./build/index.js",
    "prestart": "npm run build",
    "dev": "npx nodemon ./src/index.ts",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "sql",
    "postgresql",
    "postgres",
    "pg",
    "pgsql",
    "dbc",
    "database"
  ],
  "author": "Hugo Regibo",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "pg": "~8.8.0"
  },
  "devDependencies": {
    "@types/node": "~18.7.23",
    "typescript": "~4.8.4",
    "@types/pg": "~8.6.5"
  }
}
